{
  "name": "waymo-api",
  "version": "0.0.1",
  "description": "A nodejs service for automated posting",
  "main": "server.js",
  "scripts": {
    "start:lint": "eslint src/**/*.js",
    "start:lint:fix": "eslint src/**/*.js --fix",
    "start:nodemon": "nodemon src/server.js --exec babel-node",
    "start:build": "babel -d dist src/server.js",
    "start:server": "node dist/src/server.js",
    "start:dev": "npm run start:lint && npm run start:nodemon",
    "start:prod": "npm run start:build && npm run start:server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async-busboy": "0.4.0",
    "body-parser": "1.17.1",
    "dotenv": "4.0.0",
    "koa": "2.2.0",
    "koa-bodyparser": "4.2.0",
    "koa-multer": "1.0.1",
    "koa-router": "7.1.1",
    "request": "2.81.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-preset-env": "1.3.3",
    "eslint": "3.19.0",
    "nodemon": "1.11.0"
  }
}
